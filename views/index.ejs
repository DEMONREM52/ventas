<script>
    // Lista de sugerencias de ejemplo
    const suggestionsData = [
        "cepillo dental",
        "c칠pillo de pelo",
        "cepillo para u침as",
        "cepillo de ba침o",
        "cepillo de cocina",
        "cepillo para mascotas",
        "cepillo el칠ctrico",
        "cepillo manual",
        "cepillo de limpieza",
        "cepillo facial"
    ];

    // Elementos del DOM
    const searchInput = document.getElementById("buscador");
    const suggestionsList = document.getElementById("suggestions");

    // Evento para mostrar sugerencias al escribir
    searchInput.addEventListener("input", function () {
        const query = quitarAcentos(this.value.toLowerCase()); // Normaliza y convierte a min칰sculas

        suggestionsList.innerHTML = "";

        if (query) {
            const filteredSuggestions = suggestionsData.filter(item =>
                quitarAcentos(item.toLowerCase()).includes(query)
            );

            filteredSuggestions.forEach(item => {
                const suggestionItem = document.createElement("li");
                suggestionItem.textContent = item;
                suggestionItem.classList.add("suggestion-item");

                suggestionItem.addEventListener("click", function () {
                    searchInput.value = item;
                    suggestionsList.innerHTML = "";
                    suggestionsList.classList.remove("show");
                });

                suggestionsList.appendChild(suggestionItem);
            });

            suggestionsList.classList.add("show");
        } else {
            suggestionsList.classList.remove("show");
        }
    });

    // Funci칩n para quitar acentos y caracteres especiales
    function quitarAcentos(texto) {
        return texto.normalize("NFD").replace(/[\u0300-\u036f]/g, "");
    }

    // Evento para ocultar sugerencias al hacer clic fuera del buscador
    document.addEventListener("click", function (event) {
        if (!searchInput.contains(event.target) && !suggestionsList.contains(event.target)) {
            suggestionsList.classList.remove("show");
        }
    });
</script>


