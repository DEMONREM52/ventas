<script>
    function toggleDropdown(element) {
        var dropdownContent = element.querySelector('.dropdown-content');
        if (dropdownContent.style.display === "block") {
            dropdownContent.style.display = "none";
        } else {
            // Ocultar otros men칰s desplegables antes de mostrar este
            hideAllDropdowns();
            dropdownContent.style.display = "block";
        }
    }

    function hideAllDropdowns() {
        var allDropdowns = document.querySelectorAll('.dropdown-content');
        allDropdowns.forEach(function (dropdown) {
            dropdown.style.display = "none";
        });
    }

    // Ocultar los men칰s desplegables al cargar la p치gina
    window.addEventListener('load', function () {
        hideAllDropdowns();
    });

    // Detectar clic fuera de los men칰s desplegables para cerrarlos
    document.addEventListener('click', function (event) {
        if (!event.target.closest('.card')) {
            hideAllDropdowns();
        }
    });
</script>


<script>
    document.addEventListener("DOMContentLoaded", function () {
        // Check if there's a hash stored in localStorage
        var hash = localStorage.getItem('scrollTo');
        if (hash) {
            // Remove the item from localStorage
            localStorage.removeItem('scrollTo');

            // Get the target element
            var target = document.querySelector(hash);

            if (target) {
                // Scroll to the target element with offset
                var offset = 400; // Puedes ajustar el valor del margen aqu칤
                target.scrollIntoView({ behavior: 'smooth', block: 'start', inline: 'nearest' });

                // Scroll by additional offset
                window.scrollBy(0, -offset);

                // Apply the highlight class after a delay of 2 seconds
                setTimeout(function () {
                    target.classList.add('highlight');

                    // Optionally, remove the highlight class after animation ends
                    setTimeout(function () {
                        target.classList.remove('highlight');
                    }, 1500); // Match the duration of the CSS animation
                }, 1000); // 2 seconds delay
            }
        }
    });

</script>

